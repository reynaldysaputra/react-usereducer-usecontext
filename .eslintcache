ayload : {name : name}})\n      setName('');\n   }\n\n   const handleComplete = (item) => {   \n      dispatch({type : ACTION.COMPLETE_TODO, payload : {id : item}})\n   }\n\n   const handleDelete = (item) => {\n      dispatch({type : ACTION.DELETE_TODO, payload : {id : item}})\n   }\n   \n   return(\n      <Fragment>\n         <form onSubmit={handleSubmit}>\n            <input type=\"text\" value={name} onChange={e => setName(e.target.value)} />\n            <button type=\"submit\">Submit</button>\n         </form>\n\n         <ul>\n            {todo.map(item => (\n               <li key={item.id} style={{textDecoration: item.complete === true ? 'line-through' : 'underline'}}>\n                  {item.name}\n                  <button \n                     style={{color:'blue',cursor:'pointer',marginLeft:'10%'}}\n                     onClick={() => handleComplete(item.id)}\n                  >complete</button>\n                  <button\n                     style={{color:'red',cursor:'pointer'}}\n                     onClick={() => handleDelete(item.id)}\n                  >\n                     Delete\n                  </button>\n               </li>\n            ))}\n         </ul>\n         {console.log(todo)}\n      </Fragment>\n   )\n}\n\nexport default AppsTodo;","/Volumes/DATA/BasicDeveloper/Advance/Framework/ReactJS/LATIHAN/09-usereducer-usecontext/src/02_useContext/01_basic/parent.jsx",[],"/Volumes/DATA/BasicDeveloper/Advance/Framework/ReactJS/LATIHAN/09-usereducer-usecontext/src/02_useContext/01_basic/contextHook.jsx",[],"/Volumes/DATA/BasicDeveloper/Advance/Framework/ReactJS/LATIHAN/09-usereducer-usecontext/src/02_useContext/01_basic/buttons.jsx",[],"/Volumes/DATA/BasicDeveloper/Advance/Framework/ReactJS/LATIHAN/09-usereducer-usecontext/src/02_useContext/01_basic/box.jsx",[],{"ruleId":"41","severity":1,"message":"42","line":4,"column":8,"nodeType":"43","messageId":"44","endLine":4,"endColumn":11},{"ruleId":"41","severity":1,"message":"45","line":6,"col