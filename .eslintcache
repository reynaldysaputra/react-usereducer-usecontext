ntext/src/01_useReducer/01_todo/apps.jsx",["94"],"import React, { Fragment, useReducer, useState } from 'react';\n\nconst ACTION = {\n   ADD_TODO : 'add',\n   COMPLETE_TODO : 'complete',\n   DELETE_TODO : 'delete'\n}\n\nfunction reduceTodo(data,action){\n   switch(action.type){\n      case ACTION.ADD_TODO : \n         return [...data, {id : Date.now(), name: action.payload.name, complete : false}];\n      case ACTION.COMPLETE_TODO : \n         return data.map(item => {\n            if(item.id === action.payload.id){\n               return {...item, complete : !item.complete}\n            }\n            return item;\n         })\n      case ACTION.DELETE_TODO : \n         return data.filter(item => item.id !== action.payload.id);\n   }\n}\n\nfunction AppsTodo() {\n   const [todo, dispatch] = useReducer(reduceTodo, []);\n   const [name, setName] = useState('');\n\n   const handleSubmit = (e) => {\n      e.preventDefault();\n      dispatch({type : ACTION.ADD_TODO, payload : {name : name}})\n      setName('');\n   }\n\n   const handleComplete = (item) => {   \n      dispatch({type : ACTION.COMPLETE_TODO, payload : {id : item}})\n   }\n\n   const handleDelete = (item) => {\n      dispatch({type : ACTION.DELETE_TODO, payload : {id : item}})\n   }\n   \n   return(\n      <Fragment>\n         <form onSubmit={handleSubmit}>\n            <input type=\"text\" value={name} onChange={e => setName(e.target.value)} />\n            <button type=\"submit\">Submit</button>\n         </form>\n\n         <ul>\n            {todo.map(item => (\n               <li key={item.id} style={{textDecoration: item.complete === true ? 'line-through' : 'underline'}}>\n                  {item.name}\n                  <button \n                     style={{color:'blue',cursor:'pointer',marginLeft:'10%'}}\n                     onClick={() => handleComplete(item.id)}\n                  >complete</button>\n                  <button\n                     style={{color:'red',cursor:'pointer'}}\n                     onClick={() => handleDelete(item.id)}\n                  >\n                     Delete\n                  </button>\n               </li>\n            ))}\n         </ul>\n      </Fragment>\n   )\n}\n\nexport default AppsTodo;","/Volumes/DATA/BasicDeveloper/Advance/Framework/ReactJS/LATIHAN/09-usereducer-usecontext/src/02_useContext/01_basic/parent.jsx",[],"/Volumes/DATA/BasicDeveloper/Advance/Framework/ReactJS/LATIHAN/09-usereducer-usecontext/src/02_useContext/01_basic/contextHook.jsx",[],"/Volumes/DATA/BasicDeveloper/Advance/Framework/ReactJS/LATIHAN/09-usereducer-usecontext/src/02_useContext/01_basic/buttons.jsx",[],"/Volumes/DATA/BasicDeveloper/Advance/Framework/ReactJS/LATIHAN/09-usereducer-usecontext/src/02_useContext/01_basic/box.jsx",[],"/Volumes/DATA/BasicDeveloper/Advance/Framework/ReactJS/LATIHAN/09-usereducer-usecontext/src/02_useContext/02_todoBasic/route.js",[],"/Volumes/DATA/BasicDeveloper/Advance/Framework/ReactJS/LATIHAN/09-usereducer-usecontext/src/02_useContext/02_todoBasic/home.jsx",[],"/Volumes/DATA/BasicDeveloper/Advance/Framework/ReactJS/LATIHAN/09-usereducer-usecontext/src/02_useContext/02_todoBasic/customContext.jsx",["95","96"],"import React, { useContext, useEffect, useReducer, useState } from 'react';\n\nconst KeyTodo = {\n   ADD_DATA : 'add',\n   UPDATE_DATA : 'update',\n   DELETE_DATA : 'delete',\n   KEYLOCAL : 'dataTodo'\n}\nconst DataContext = React.createContext();\nconst AddDataContext = React.createContext();\nconst localStorageData = JSON.parse(localStorage.getItem(KeyTodo.KEYLOCAL));\nconst temporaryData = localStorageData != null ? localStorageData : [];\n\nfunction Reduce(data,action) {\n   switch(action.type) {\n      case KeyTodo.ADD_DATA : \n         temporaryData.push({id : Math.random(), name : action.payload.dataName});\n         localStorage.setItem(KeyTodo.KEYLOCAL, JSON.stringify(temporaryData));         \n         return JSON.parse(localStorage.getItem(KeyTodo.KEYLOCAL));\n      case KeyTodo.DELETE_DATA : \n         temporaryData.find((item,index) => item.id === action.payload.id && temporaryData.splice(index,1));\n         localStorage.removeItem(KeyTodo.KEYLOCAL);\n         localStorage.setItem(KeyTodo.KEYLOCAL, JSON.stringify(temporaryData))\n         return JSON.parse(localStorage.getItem(KeyTodo.KEYLOCAL));\n      case KeyTodo.UPDATE_DATA :     \n         console.log(JSON.parse(localStorage.getItem(KeyTodo.KEYLOCAL)));     \n         return temporaryData;\n      default : \n         return data;\n   }\n}\n\nexport function UseData() {return useContext(DataContext)};\nexport function UsePost() {return useContext(AddDataContext)}\n\nexport function TodoContext({children}) {\n   const [data, dispatch] = useReducer(Reduce, temporaryData);\n\n   console.log('render custom context');\n\n   const Action = (value, dataAction) => {\n      const actionKey = dataAction.target.dataset.action;\n      \n      if(KeyTodo.ADD_DATA === actionKey) {\n         dispatch({type : KeyTodo.ADD_DATA, payload : {dataName: value}});\n      }else if (KeyTodo.DELETE_DATA === actionKey) {\n         dispatch({type : KeyTodo.DELETE_DATA, payload: {id : Number(value)}})\n      }else {\n         dispatch({type : KeyTodo.UPDATE_DATA, payload: {id : Number(value)}})\n      }\n   }\n\n   return(\n      <DataContext.Provider value={data}>\n         <AddDataContext.Provider value={Action}>\n            {children}\n         </AddDataContext.Provider>\n      </DataContext.Provider>\n   )\n}\n\n","/Volumes/DATA/BasicDeveloper/Advance/Framework/ReactJS/LATIHAN/09-usereducer-usecontext/src/02_useContext/02_todoBasic/form.jsx",["97"],"import React, { Fragment, useState } from 'react';\nimport { UseData, UsePost } from './customContext';\n\nfunction Form() {\n   const [name, setName] = useState('');\n   const Post = UsePost();\n   const data = UseData();\n\n   const ActionButton = (e) => {\n      e.preventDefault(); \n      Post(name, e); \n      setName('');\n   }\n\n   return(\n      <Fragment>\n         {console.log('render form')}\n         <form onSubmit={ActionButton} data-action='add'>\n            <input type=\"text\" value={name}  onChange={(e) => setName(e.target.value)} />\n            <button type=\"submit\">Add</button>\n         </form>\n\n      </Fragment> \n   )\n}\n\nexport default Form;","/Volumes/DATA/BasicDeveloper/Advance/Framework/ReactJS/LATIHAN/09-usereducer-usecontext/src/02_useContext/02_todoBasic/todo.jsx",[],"/Volumes/DATA/BasicDeveloper/Advance/Framework/ReactJS/LATIHAN/09-usereducer-usecontext/src/02_useContext/02_todoBasic/dataBox.jsx",["98","99"],"import React, { Fragment, useEffect, useState } from 'react';\nimport { UseData, UsePost } from './customContext';\n\nfunction DataBox(){\n   const data = UseData();\n   const post = UsePost();\n   const ActionButton = (e) =>{\n      post(e.target.parentElement.dataset.id, e);\n   };\n\n   return(\n      <Fragment>\n         <h1>Data Pelanggan</h1>   \n         {console.log('render databox')}\n         {data.map(item => (\n            <div key={item.id} style={{display:'flex',marginLeft:'10px'}} data-id={item.id}>\n               <p style={{marginRight : '20px'}}>{item.name}</p>\n               <button \n                  type=\"submit\" \n                  style={{cursor:'pointer'}} \n                  data-action='update' \n                  onClick={ActionButton}>Update\n               </button>\n               <button \n                  type=\"submit\" \n                  style={{cursor:'pointer'}} \n                  data-action='delete' \n                  onClick={ActionButton}>Hapus\n               </button>\n            </div>\n         ))}\n      </Fragment>\n   )\n}\n\nexport default DataBox;","/Volumes/DATA/BasicDeveloper/Advance/Framework/ReactJS/LATIHAN/09-usereducer-usecontext/src/LATIHAN/01_ecommerceBasic/apps.js",["100"],"/Volumes/DATA/BasicDeveloper/Advance/Framework/ReactJS/LATIHAN/09-usereducer-usecontext/src/LATIHAN/01_ecommerceBasic/component/nav.jsx",[],"/Volumes/DATA/B